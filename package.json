{
  "name": "gampang",
  "version": "1.6.10",
  "description": "\"Gampang\" is a whatsapp bot framework. Based on @adiwajshing/baileys",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "format": "prettier . --write",
    "lint": "eslint \"src/**/*.{ts,js,d.ts}\" --fix",
    "build": "tsup",
    "test": "nodemon ./examples/normal.ts",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "docs": "rimraf docs && typedoc --options typedoc.json"
  },
  "keywords": [
    "baileys",
    "gampang",
    "whatsapp-bot"
  ],
  "author": "Hanif Dwy Putra S <hanifdwyputrasembiring@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/qrcode": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "eslint": "^8.37.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "sharp": "^0.31.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^6.7.0",
    "typedoc": "^0.23.28",
    "typedoc-plugin-mdn-links": "^2.0.2",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@adiwajshing/baileys": "^4.4.0",
    "@peculiar/webcrypto": "^1.4.3",
    "long": "^5.2.1",
    "pino": "^8.11.0",
    "pino-pretty": "^9.4.0",
    "qrcode": "^1.5.1"
  },
  "nodemonConfig": {
    "ignore": [
      "./examples/sessions/*"
    ],
    "verbose": true,
    "env": {
      "NODE_ENV": "development"
    },
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "./examples/",
      "./src/"
    ]
  },
  "repository": {
    "url": "https://github.com/hansputera/gampang.git"
  },
  "engines": {
    "node": ">=12.x"
  }
}
