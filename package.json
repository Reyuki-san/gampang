{
  "name": "gampang",
  "version": "1.5.1",
  "description": "\"Gampang\" is a whatsapp bot framework. Based on @adiwajshing/baileys",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "format": "prettier . --write",
    "lint": "eslint \"src/**/*.{ts,js,d.ts}\" --fix",
    "build": "tsup",
    "test": "npm run build && nodemon ./examples/qr_test.js",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "docs": "rimraf docs && typedoc --options typedoc.json"
  },
  "keywords": [
    "baileys",
    "gampang",
    "whatsapp-bot"
  ],
  "author": "Hanif Dwy Putra S <hanifdwyputrasembiring@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^18.11.4",
    "@types/qrcode": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "eslint": "^8.26.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sharp": "^0.31.1",
    "tsup": "^6.3.0",
    "typedoc": "^0.23.18",
    "typedoc-plugin-mdn-links": "^2.0.0",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@adiwajshing/baileys": "github:hansputera/Baileys",
    "long": "^5.2.0",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "qrcode": "^1.5.1"
  },
  "nodemonConfig": {
    "ignore": [
      "./examples/sessions/*"
    ]
  },
  "repository": {
    "url": "https://github.com/hansputera/gampang.git"
  }
}
