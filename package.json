{
  "name": "gampang",
  "version": "1.1.0-beta.5",
  "description": "\"Gampang\" is a whatsapp bot framework. Based on @adiwajshing/bailey",
  "main": "./dist/index.js",
  "scripts": {
    "format": "prettier . --write",
    "lint": "eslint \"src/**/*.{ts,js,d.ts}\" --fix",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && nodemon ./examples/qr_test.js",
    "clean": "rimraf dist",
    "prepare": "husky install"
  },
  "keywords": [
    "baileys",
    "gampang",
    "whatsapp-bot"
  ],
  "author": "Hanif Dwy Putra S <hanifdwyputrasembiring@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^18.7.2",
    "@types/qrcode": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "5.33.1-alpha.6",
    "@typescript-eslint/parser": "5.33.1-alpha.6",
    "eslint": ">=5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sharp": "^0.31.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@adiwajshing/baileys": "^4.3.0",
    "long": "^5.2.0",
    "pino": "^8.4.1",
    "pino-pretty": "^9.0.0",
    "qrcode": "^1.5.1"
  },
  "nodemonConfig": {
    "ignore": [
      "./examples/sessions/*"
    ]
  }
}
